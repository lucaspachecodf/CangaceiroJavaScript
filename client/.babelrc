{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "entry"
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            },
            "babel-plugin-syntax-dynamic-import"
        ],
        [
            "polyfill-regenerator",
            {
                "method": "usage-global"
            }
        ]
    ]
}